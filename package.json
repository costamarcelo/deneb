{
    "name": "deneb",
    "license": "MIT",
    "scripts": {
        "pbiviz": "pbiviz",
        "pbiviz-version": "pbiviz --version",
        "install-cert": "pbiviz --install-cert",
        "start": "npm run template-schemagen && pbiviz start",
        "package": "npm run validate-config-for-commit && npm run template-schemagen && pbiviz package",
        "package-standalone": "npm run template-schemagen && node bin/package-custom --mode standalone",
        "package-alpha": "npm run template-schemagen && node bin/package-custom --mode alpha",
        "package-beta": "npm run template-schemagen && node bin/package-custom --mode beta",
        "eslint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
        "prettier-check": "prettier --config .prettierrc {src,spec,style}/**/{*.ts*,*.css,*.less} --check",
        "prettier-format": "prettier --config .prettierrc {src,spec,style}/**/{*.ts*,*.css,*.less} --write",
        "template-schemagen": "typescript-json-schema ./src/features/template/schema.ts IDenebTemplateMetadata -o ./schema/deneb-template-usermeta-v1.json --required --topRef",
        "validate-config-for-commit": "node bin/validate-config-for-commit"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/runtime": "^7.14.8",
        "@babel/runtime-corejs3": "^7.14.9",
        "@fluentui/react-components": "^9.26.2",
        "@fluentui/react-icons": "^2.0.206",
        "@types/ace": "^0.0.44",
        "@types/d3": "^7.1.0",
        "@types/jsoneditor": "^8.6.0",
        "@types/jsum": "^0.1.0",
        "@types/lodash": "^4.14.168",
        "@types/node": "^16.4.12",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "ace-builds": "^1.4.13",
        "adm-zip": "^0.5.5",
        "ajv": "^6.12.6",
        "axios": "^0.21.2",
        "core-js": "^3.16.0",
        "d3": "7.2.1",
        "d3-format": "^2.0.0",
        "d3-time-format": "^3.0.0",
        "eslint": "^8.41.0",
        "eslint-plugin-powerbi-visuals": "^0.8.1",
        "git-last-commit": "^1.0.1",
        "json-stringify-pretty-compact": "^3.0.0",
        "jsoneditor": "^9.7.4",
        "jsonrepair": "^2.2.1",
        "jsum": "^2.0.0-alpha.4",
        "lodash": "^4.17.21",
        "minimist": "^1.2.5",
        "powerbi-visuals-api": "^5.3.0",
        "powerbi-visuals-tools": "^4.3.1",
        "powerbi-visuals-utils-dataviewutils": "^6.0.1",
        "powerbi-visuals-utils-formattingutils": "^6.0.1",
        "powerbi-visuals-utils-interactivityutils": "^6.0.2",
        "prettier": "^2.2.1",
        "process": "^0.11.10",
        "react": "^17.0.1",
        "react-ace": "^9.4.3",
        "react-custom-scrollbars-2": "^4.4.0",
        "react-data-table-component": "^7.5.3",
        "react-dom": "^17.0.1",
        "react-dropzone": "^14.2.3",
        "react-hotkeys-hook": "^3.4.0",
        "react-konami-code": "^2.3.0",
        "react-split-pane": "^0.1.92",
        "react-tabulator": "^0.18.1",
        "react-vega": "7.6.0",
        "styled-components": "^6.0.0-rc.3",
        "ts-loader": "^9.2.5",
        "typescript": "^4.2.4",
        "typescript-json-schema": "^0.50.0",
