{
  "name": "@deneb-viz/template-usermeta-schema",
  "version": "1.7.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deneb-viz/sadr.git"
