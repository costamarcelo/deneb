{
    "name": "@deneb-viz/core-dependencies",
    "version": "1.7.0",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/deneb-viz/sadr.git"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "clean": true,
        "dts": true,
        "format": [
            "cjs",
            "esm"
        ]
    },
    "scripts": {
        "dev": "tsup-node --watch",
        "build": "tsup-node --dts",
        "test": "jest",
        "test:watch": "jest --watch",
        "eslint": "eslint . --max-warnings 0"
    },
    "jest": {
        "preset": "@deneb-viz/jest-presets/jest/node"
    },
    "devDependencies": {
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/jest-presets": "*",
        "@deneb-viz/typescript-config": "*",
        "@types/uuid": "^9.0.7",
        "powerbi-visuals-api": "5.7.0",
        "uuid": "^9.0.1",
        "vega": "5.28.0",
        "vega-lite": "5.18.0"
    }
}
